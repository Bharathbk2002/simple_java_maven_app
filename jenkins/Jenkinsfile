pipeline
{
    agent any
    tools {
  maven 'my-maven'
}
    stages
    {
        stage('build') 
        {
             steps 
             {
                sh 'mvn clean pacakge'
             }
        post 
        {
            success 
            {
                archiveArtifacts artifacts: '**/target/*.war'
            }
        }
        }

   }
}